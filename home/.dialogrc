#
# Run-time configuration file for dialog
#
# Generated by "dialog --create-rc <file>"

# Set aspect-ration.
aspect = 0

# Set separator (for multiple widgets output).
separate_widget = ""

# Set tab-length (for textbox tab-conversion).
tab_len = 0

# Make tab-traversal for checklist, etc., include the list.
visit_items = OFF

# Shadow dialog boxes? This also turns on color.
use_shadow = ON

# Turn color support ON or OFF
use_colors = ON

# Screen color
screen_color = (WHITE,WHITE,ON)

# Shadow color
shadow_color = (WHITE,WHITE,ON)

# Dialog box color
dialog_color = (BLACK,WHITE,OFF)

# Dialog box title color
title_color = (BLACK,WHITE,ON)

# Dialog box border color
border_color = (BLACK,WHITE,ON)

# Active button color
button_active_color = (BLACK,BLACK,OFF)

# Inactive button color
button_inactive_color = (WHITE, WHITE, OFF)

# Active button key color
button_key_active_color = (RED, BLACK, OFF)

# Inactive button key color
button_key_inactive_color = (RED, WHITE, OFF)

# Active button label color
button_label_active_color = (WHITE,BLACK,OFF)

# Inactive button label color
button_label_inactive_color = (BLACK, WHITE, OFF)

# Input box color
inputbox_color = (BLACK, WHITE, OFF)

# Input box border color
inputbox_border_color = (BLACK, BLACK, OFF)

# Search box color
searchbox_color = dialog_color

# Search box title color
searchbox_title_color = title_color

# Search box border color
searchbox_border_color = border_color

# File position indicator color
position_indicator_color = title_color

# Menu box color
menubox_color = dialog_color

# Menu box border color
menubox_border_color = border_color

# Item color
item_color = (BLACK, WHITE, OFF)

# Selected item color
item_selected_color = (WHITE, BLACK, OFF)

# Tag color
tag_color = item_color

# Selected tag color
tag_selected_color = item_selected_color

# Tag key color
tag_key_color = button_key_inactive_color

# Selected tag key color
tag_key_selected_color = (RED,BLACK,ON)

# Check box color
check_color = item_color

# Selected check box color
check_selected_color = item_selected_color

# Up arrow color
uarrow_color = (GREEN,WHITE,ON)

# Down arrow color
darrow_color = uarrow_color

# Item help-text color
itemhelp_color = (BLACK,WHITE, OFF)

# Active form text color
form_active_text_color = (BLACK, WHITE, OFF)

# Form text color
form_text_color = (BLACK,CYAN,ON)

# Readonly form item color
form_item_readonly_color = (CYAN,WHITE,ON)

# Dialog box gauge color
gauge_color = title_color

# Dialog box border2 color
border2_color = dialog_color

# Input box border2 color
inputbox_border2_color = (BLACK, BLACK, OFF)

# Search box border2 color
searchbox_border2_color = dialog_color

# Menu box border2 color
menubox_border2_color = dialog_color
